@article{xuGISbasedBivariateStatistical2013,
  title = {{{GIS}}-Based Bivariate Statistical Modelling for Earthquake-Triggered Landslides Susceptibility Mapping Related to the 2008 {{Wenchuan}} Earthquake, {{China}}},
