@article{linImpactChiChiEarthquake2004,
  title = {Impact of {{Chi}}-{{Chi}} Earthquake on the Occurrence of Landslides and Debris Flows: Example from the {{Chenyulan River}} Watershed, {{Nantou}}, {{Taiwan}}},
  shorttitle = {Impact of {{Chi}}-{{Chi}} Earthquake on the Occurrence of Landslides and Debris Flows},
  author = {Lin, Ching-Weei and Shieh, Chjeng-Lun and Yuan, Bee-Deh and Shieh, Yeou-Chung and Liu, Shou-Heng and Lee, Sen-Yuan},
  year = {2004},
  month = jan,
  volume = {71},
  pages = {49--61},
  issn = {0013-7952},
  doi = {10.1016/S0013-7952(03)00125-X},
  abstract = {The Chenyulan River watershed in central Taiwan was chosen for evaluating the impact of the Chi-Chi earthquake on the occurrence of landslides, and for exploring the initial conditions triggering debris flows. Interpretations of aerial photographs and SPOT images as well as field investigations were used to identify landslide and debris flow occurrences. Precipitation data were then utilized to investigate critical conditions leading to the debris flow. Comparison of five SPOT images taken between June 1996 (before the July 1996 Typhoon Herb) and September 1999 (before the Chi-Chi earthquake) shows that the landslide area increased from 7.1\texttimes 106 to 7.5\texttimes 106 m2 during that time. However, by January 2000 (after the Chi-Chi earthquake), the landslide area almost tripled to 20.8\texttimes 106 m2. Measurement of images taken in March and August 2001 reveals that the landslide area had further expanded to 24.2\texttimes 106 and 27.5\texttimes 106 m2, respectively. Significant differences before and after the earthquake are also noticed in (1) the intensity and amount of precipitation required for triggering debris flow, (2) the size of contributing drainage basin in which debris flows occurred, and (3) the frequency of debris flows. After the Chi-Chi earthquake, maximum hourly rainfall intensity and critical accumulated precipitation necessary to initiate debris flow reduced to as low as 1/3 of the pre-earthquake figures. Prior to the Chi-Chi earthquake, most debris flows occurred in gullies with slopes greater than 10\textdegree, and in drainage basins larger than 0.1 km2. Conversely, after the earthquake, debris flows were observed even in gullies with effective drainage area smaller than 0.03 km2. Furthermore, before the earthquake, the debris flow recurrence time in the study area was greater than 5 years, whereas six debris flow events have been observed in the 2 years since the earthquake.},
  file = {/Users/elsaculler/GoogleDrive/zotero_storage/Landslides/Lin et al. - 2004 - Impact of Chi-Chi earthquake on the occurrence of .pdf},
  journal = {Engineering Geology},
  keywords = {_tablet,Chenyulan River,Chi-Chi earthquake,Debris flow,Landslide,Taiwan},
  number = {1},
  series = {Reconnaissance of the {{Chi}}-{{Chi Earthquakes}}, {{Taiwan}}}
}

@article{linImpactsChiChiEarthquake2006,
  title = {Impacts of the {{Chi}}-{{Chi}} Earthquake on Subsequent Rainfall-Induced Landslides in Central {{Taiwan}}},
