@article{saxtonSoilWaterCharacteristic2006,
  title = {Soil {{Water Characteristic Estimates}} by {{Texture}} and {{Organic Matter}} for {{Hydrologic Solutions}}},
  author = {Saxton, K. E. and Rawls, W. J.},
  year = {2006},
  volume = {70},
