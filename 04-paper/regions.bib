@article{meenaComparisonEarthquakeTriggeredLandslide2019,
  title = {Comparison of {{Earthquake}}-{{Triggered Landslide Inventories}}: {{A Case Study}} of the 2015 {{Gorkha Earthquake}}, {{Nepal}}},
