@article{akgunLandslideSusceptibilityMapping2008,
  title = {Landslide Susceptibility Mapping for a Landslide-Prone Area ({{Findikli}}, {{NE}} of {{Turkey}}) by Likelihood-Frequency Ratio and Weighted Linear Combination Models},
  author = {Akgun, Aykut and Dag, Serhat and Bulut, Fikri},
