@article{fourniadisLandslideHazardAssessment2007,
  title = {Landslide Hazard Assessment in the {{Three Gorges}} Area, {{China}}, Using {{ASTER}} Imagery: {{Wushan}}\textendash{{Badong}}},
  shorttitle = {Landslide Hazard Assessment in the {{Three Gorges}} Area, {{China}}, Using {{ASTER}} Imagery},
  author = {Fourniadis, I. G. and Liu, J. G. and Mason, P. J.},
  year = {2007},
  month = feb,
  volume = {84},
