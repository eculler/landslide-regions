@article{clericiProcedureLandslideSusceptibility2002,
  title = {A Procedure for Landslide Susceptibility Zonation by the Conditional Analysis Method},
  author = {Clerici, Aldo and Perego, Susanna and Tellini, Claudio and Vescovi, Paolo},
