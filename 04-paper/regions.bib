@article{regmiApplicationFrequencyRatio2014,
  title = {Application of Frequency Ratio, Statistical Index, and Weights-of-Evidence Models and Their Comparison in Landslide Susceptibility Mapping in {{Central Nepal Himalaya}}},
  author = {Regmi, Amar Deep and Devkota, Krishna Chandra and Yoshida, Kohki and Pradhan, Biswajeet and Pourghasemi, Hamid Reza and Kumamoto, Takashi and Akgun, Aykut},
  year = {2014},
