@article{tunusluogluArtificialNeuralNetwork2007,
  title = {An Artificial Neural Network Application to Produce Debris Source Areas of {{Barla}}, {{Besparmak}}, and {{Kapi Mountains}} ({{NW Taurids}}, {{Turkey}})},
