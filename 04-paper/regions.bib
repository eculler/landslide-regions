@article{galantiComparisonStatisticalMethods2018,
  title = {Comparison of Statistical Methods and Multi-Time Validation for the Determination of the Shallow Landslide Rainfall Thresholds},
  author = {Galanti, Yuri and Barsanti, Michele and Cevasco, Andrea and D'Amato Avanzi, Giacomo and Giannecchini, Roberto},
