@article{degraffTimingSusceptibilityPostFire2015,
  title = {The {{Timing}} of {{Susceptibility}} to {{Post}}-{{Fire Debris Flows}} in the {{Western United States}}},
  author = {DeGraff, Jerome V. and Cannon, Susan H. and Gartner, Joseph E.},
  year = {2015},
  month = nov,
  volume = {21},
