  title = {Extraction of Potential Debris Source Areas by Logistic Regression Technique: A Case Study from {{Barla}}, {{Besparmak}} and {{Kapi}} Mountains ({{NW Taurids}}, {{Turkey}})},
  shorttitle = {Extraction of Potential Debris Source Areas by Logistic Regression Technique},
  author = {Tunusluoglu, M. C. and Gokceoglu, C. and Nefeslioglu, H. A. and Sonmez, H.},
