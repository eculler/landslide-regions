  journal = {Journal of Hydrology},
  keywords = {Drought,Land surface modeling,Pacific Northwest,Uncertainty,VIC},
  language = {en}
}

@article{akgunApplicationRemoteSensing2012,
  title = {Application of Remote Sensing Data and {{GIS}} for Landslide Risk Assessment as an Environmental Threat to {{Izmir}} City (West {{Turkey}})},
  author = {Akgun, Aykut and K{\i}ncal, Cem and Pradhan, Biswajeet},
  year = {2012},
