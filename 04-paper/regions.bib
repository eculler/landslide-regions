  journal = {International Journal of Climatology},
  keywords = {Benevento,elevation,GIS,multivariate geostatistics,precipitation,southern Italy,vegetation cover factor},
  language = {en},
  number = {3}
}

@article{donatiObjectiveMethodRank2002,
