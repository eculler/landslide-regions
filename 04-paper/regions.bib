@article{wuDeterminingLandslideSusceptibility2009,
  title = {Determining Landslide Susceptibility in {{Central Taiwan}} from Rainfall and Six Site Factors Using the Analytical Hierarchy Process Method},
  author = {Wu, Chun-Hung and Chen, Su-Chin},
  year = {2009},
  month = nov,
  volume = {112},
