@article{keanSituMeasurementsPostfire2018,
  title = {In Situ Measurements of Post-Fire Debris Flows in Southern {{California}}: {{Comparisons}} of the Timing and Magnitude of 24 Debris-Flow Events with Rainfall and Soil Moisture Conditions},
  shorttitle = {In Situ Measurements of Post-Fire Debris Flows in Southern {{California}}},
  author = {Kean, Jason W. and Staley, Dennis M. and Cannon, Susan H.},
  year = {2018},
