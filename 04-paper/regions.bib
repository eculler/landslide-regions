@incollection{santi2020wildfire,
title = {Wildfire and Landscape Change☆},
booktitle = {Reference Module in Earth Systems and Environmental Sciences},
publisher = {Elsevier},
year = {2020},
isbn = {978-0-12-409548-9},
doi = {https://doi.org/10.1016/B978-0-12-818234-5.00017-1},
url = {https://www.sciencedirect.com/science/article/pii/B9780128182345000171},
author = {Paul M. Santi and Francis K. Rengers},
keywords = {Aggradation, Ash, Burn, Debris flow, Dry ravel, Erosion, Fire, Hydrophobic, Incision, Infiltration, Landslides, Weathering, Wildfire},
abstract = {Wildfire is a worldwide phenomenon that is expected to increase in extent and severity in the future, due to fuel accumulations, shifting land management practices, and climate change. It immediately affects the landscape by removing vegetation, depositing ash, influencing water-repellent soil formation, and physically weathering boulders and bedrock. These changes typically lead to increased erosion through sheetwash, rilling, dry ravel, and increased mass movement in the form of floods, debris flow, rockfall, and landslides. These process changes bring about landform changes as hillslopes are lowered and stream channels aggrade or incise at increased rates. Furthermore, development of alluvial fans, debris fans, and talus cones are enhanced. The window of disturbance to the landscape caused by wildfire is typically on the order of 3–4years, with some effects persisting up to 30years.}
}

