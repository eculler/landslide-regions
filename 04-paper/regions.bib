@article{dammLandslideDatabaseGermany2015,
  title = {The Landslide Database for {{Germany}}: {{Closing}} the Gap at National Level},
  shorttitle = {The Landslide Database for {{Germany}}},
  author = {Damm, Bodo and Klose, Martin},
  year = {2015},
  month = nov,
  volume = {249},
